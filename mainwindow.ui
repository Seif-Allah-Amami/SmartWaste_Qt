   <string notr="true">/* ===== Global ===== */
QWidget {
    background-color: #050d0a;
    color: #66ffcc;
    font-family: &quot;Segoe UI&quot;, &quot;Inter&quot;, &quot;Arial&quot;;
    font-size: 13px;
}

/* ===== Main Content Area ===== */
QWidget#centralwidget {
    background-color: #050d0a;
}

/* ===== Sidebar ===== */
QGroupBox#sidebarGroup {
    background-color: #0a1f18;
    border: none;
    border-right: 2px solid #00ff9c;
}

QLabel#sidebarLogo {
    background: transparent;
    border: none;
    padding: 20px;
}

QLabel#sidebarTitle {
    font-size: 22px;
    font-weight: bold;
    color: #00ff9c;
    background: transparent;
    border: none;
    padding: 10px 20px;
}

/* Sidebar Menu Buttons */
QPushButton#sidebarBtn {
    background-color: transparent;
    border: none;
    border-radius: 8px;
    padding: 14px 20px;
    color: #66ffcc;
    text-align: left;
    font-size: 14px;
    font-weight: 500;
}

QPushButton#sidebarBtn:hover {
    background-color: rgba(0, 255, 156, 0.1);
    color: #00ff9c;
}

/* Active sidebar button */
QPushButton#sidebarBtnActive {
    background-color: rgba(0, 255, 156, 0.15);
    border: none;
    border-radius: 8px;
    padding: 14px 20px;
    color: #00ff9c;
    text-align: left;
    font-size: 14px;
    font-weight: 600;
    border-left: 3px solid #00ff9c;
}

/* Logout button */
QPushButton#logoutBtn {
    background-color: transparent;
    border: 1px solid #ff6666;
    border-radius: 8px;
    padding: 12px 20px;
    color: #ff6666;
    font-weight: 600;
}

QPushButton#logoutBtn:hover {
    background-color: rgba(255, 102, 102, 0.1);
}

/* ===== Login/Auth Pages ===== */
QLabel#authTitle {
    font-size: 28px;
    font-weight: 700;
    color: #ffffff;
    background: transparent;
    border: none;
}

QLabel#authSubtitle {
    font-size: 14px;
    font-weight: 400;
    color: #66ffcc;
    background: transparent;
    border: none;
}

QLabel#authLogo {
    background: transparent;
    border: none;
}

/* ===== Page Header ===== */
QLabel#pageTitle {
    font-size: 28px;
    font-weight: 700;
    color: #ffffff;
    background: transparent;
    border: none;
}

QLabel#pageSubtitle {
    font-size: 13px;
    font-weight: 400;
    color: #66ffcc;
    background: transparent;
    border: none;
}

/* ===== Buttons ===== */
QPushButton {
    background-color: #0a1f18;
    border: 1px solid #00ff9c;
    border-radius: 10px;
    padding: 10px 20px;
    color: #66ffcc;
    font-weight: 600;
}

QPushButton:hover {
    background-color: rgba(0, 255, 156, 0.1);
    border: 1px solid #33ffcc;
}

QPushButton:pressed {
    background-color: #05140f;
}

QPushButton#primaryBtn {
    background-color: #00ff9c;
    border: 1px solid #00ff9c;
    color: #050d0a;
}

QPushButton#primaryBtn:hover {
    background-color: #33ffcc;
}

QPushButton#linkBtn {
    background-color: transparent;
    border: none;
    color: #00ff9c;
    text-decoration: underline;
}

QPushButton#linkBtn:hover {
    color: #33ffcc;
}

/* ===== Inputs ===== */
QLineEdit {
    background-color: #081713;
    border: 1px solid #00ff9c;
    border-radius: 8px;
    padding: 10px 15px;
    color: #66ffcc;
    font-size: 13px;
}

QLineEdit:focus {
    border: 1px solid #33ffcc;
    background-color: #0a1f18;
}

QLineEdit::placeholder {
    color: #4d9977;
}

QComboBox {
    background-color: #081713;
    border: 1px solid #00ff9c;
    border-radius: 8px;
    padding: 10px 15px;
    color: #66ffcc;
    font-size: 13px;
}

QComboBox:hover {
    border: 1px solid #33ffcc;
}

/* ===== Table ===== */
QTableWidget {
    background-color: #081713;
    border: 1px solid #00ff9c;
    border-radius: 10px;
    gridline-color: rgba(0, 255, 156, 0.2);
    padding: 5px;
}

QTableWidget::item {
    padding: 10px;
    color: #66ffcc;
}

QTableWidget::item:selected {
    background-color: rgba(0, 255, 156, 0.2);
    color: #00ff9c;
}

QHeaderView::section {
    background-color: #0a1f18;
    color: #00ff9c;
    padding: 12px 10px;
    border: none;
    border-bottom: 2px solid #00ff9c;
    font-weight: 600;
    font-size: 12px;
    text-transform: uppercase;
}

/* ===== Actions Panel ===== */
QFrame#actionsPanel {
    background-color: #081713;
    border: 1px solid #00ff9c;
    border-radius: 10px;
}

QLabel#actionsPanelTitle {
    font-size: 16px;
    font-weight: 600;
    color: #ffffff;
    background: transparent;
    border: none;
    padding: 15px;
}

QPushButton#actionBtn {
    background-color: transparent;
    border: 1px solid #00ff9c;
    border-radius: 8px;
    padding: 12px 15px;
    color: #00ff9c;
    font-weight: 500;
}

QPushButton#actionBtn:hover {
    background-color: rgba(0, 255, 156, 0.1);
}

QPushButton#actionBtnDanger {
    background-color: transparent;
    border: 1px solid #ff6666;
    border-radius: 8px;
    padding: 12px 15px;
    color: #ff6666;
    font-weight: 500;
}

QPushButton#actionBtnDanger:hover {
    background-color: rgba(255, 102, 102, 0.1);
}

QPushButton#actionBtnExport {
    background-color: #00ff9c;
    border: 1px solid #00ff9c;
    border-radius: 8px;
    padding: 12px 15px;
    color: #050d0a;
    font-weight: 600;
}

QPushButton#actionBtnExport:hover {
    background-color: #33ffcc;
}

/* ===== Main Menu Cards ===== */
QPushButton#menuCard {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #0a1f18, stop:1 #081713);
    border: 2px solid #00ff9c;
    border-radius: 12px;
    padding: 30px 20px;
    color: #ffffff;
    font-size: 16px;
    font-weight: 600;
    text-align: center;
}

QPushButton#menuCard:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #10352a, stop:1 #0a1f18);
    border: 2px solid #33ffcc;
}

/* ===== Scrollbars ===== */
QScrollBar:vertical {
    background: #050d0a;
    width: 10px;
    border-radius: 5px;
}

QScrollBar::handle:vertical {
    background: #00ff9c;
    border-radius: 5px;
    min-height: 20px;
}

QScrollBar::handle:vertical:hover {
    background: #33ffcc;
}

/* ===== Status Bar ===== */
QStatusBar {
    background-color: #0a1f18;
    color: #66ffcc;
    border-top: 1px solid #00ff9c;
}
</string>
